  "license": "MIT",
  "default": "n/a",
  "scripts": {
    "ts-gatsby": "ts-node ./node_modules/.bin/gatsby",
    "develop": "yarn ts-gatsby develop",
    "dev": "yarn develop",
    "serve": "yarn ts-gatsby serve",
    "build": "yarn ts-gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "typecheck": "tsc",
    "lint:eslint": "eslint --ext .ts,.tsx .",
    "lint:prettier": "prettier --check '**/*.{ts,tsx}'",
    "lint:stylelint": "prettier --check '**/*.{css,scss}'",
    "lint:remark": "remark content/",
    "lint": "run-s lint:eslint lint:prettier lint:stylelint",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "fix:stylelint": "yarn lint:stylelint --write",
    "fix": "run-s fix:eslint fix:prettier fix:stylelint"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "browserslist": [
    "Last 2 versions"
  ]
